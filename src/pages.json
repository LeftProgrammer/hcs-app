    },
    {
      "path": "pages/login/index",
      "type": "page",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/mine/index",
      "type": "page",
      "style": {
        "navigationStyle": "custom"
      },
      "needLogin": true
    },
    {
      "path": "pages/webview/index",
      "type": "page",
      "style": {
        "navigationStyle": "custom"
      },
      "needLogin": true
    }
